read eyebar.frd

rot -z
ds -2 e 1
view disp
view elem
zoom 1.2
plot fv all
hcpy png PE

ds -4 e 7
plot fv all
hcpy png SE

# create force-displacement plot
sys dat2txt.py eyebar
sys join "total force fx,fy,fz_NCONTROL.txt" "displacements vx,vy,vz_NMONITOR.txt" > df.txt
sys gnuplot df.gnu

# path plots
read fix.nam inp
read path.nam inp
view disp off
ds 2 e 1
graph Nfix length
graph Npath length
ds -4 e 1
graph Nfix length
graph Npath length
sys gnuplot path.gnu
